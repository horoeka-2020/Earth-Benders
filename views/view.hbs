<div class="landmark">
    <h1 class="title">Auckland City Landmarks</h1>
    
        {{#each landmarkList}}
        <div class="landmark-list"></div>
        <a href="/display/{{id}}"><img class="img-circle" src="{{this.image}}" alt="{{this.name}}"></a>
        <p>Landmark Name: <a href ="/landmark/{{id}}">{{this.name}}</a></p>
        <p>Landmark Description: {{this.description}}</p>
        </div>
        {{/each}}

  {{!-- <h1>Landmark Name:  {{landmark.name}}</h1>
  <div>Landmark Description: {{landmark.description}}</div>
  <div></div> --}}
  <br>
</div>
